      </VBox>
      <HBox layoutX="-1.0" layoutY="-5.0" prefHeight="70.0" prefWidth="1000.0" style="-fx-background-color: white;">
         <children>
            <ImageView fx:id="logo" fitHeight="69.0" fitWidth="115.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../image/Logo.png" />
               </image>
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
   </children>
</AnchorPane>

                        <MenuItem mnemonicParsing="false" text="Close" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <MenuBar fx:id="medicine" layoutX="10.0" layoutY="137.0" prefHeight="50.0" prefWidth="140.0" styleClass="sideMenu">

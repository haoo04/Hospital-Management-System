                  <Menu mnemonicParsing="false">
                     <graphic>
                        <Label text="Label" textFill="WHITE" />
                     </graphic>
                     <items>
                        <MenuItem mnemonicParsing="false" text="Close" />
                     </items>
                  </Menu>
               </menus>
            </MenuBar>
            <MenuBar fx:id="staff" layoutX="10.0" layoutY="37.0" prefHeight="50.0" prefWidth="140.0" styleClass="sideMenu">
               <menus>
                  <Menu mnemonicParsing="false">
                     <graphic>
                        <Label text="Label" textFill="WHITE" />
                     </graphic>
                     <items>
                        <MenuItem mnemonicParsing="false" text="Close" />

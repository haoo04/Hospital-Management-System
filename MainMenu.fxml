<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="650.0" prefWidth="1000.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainMenu">
   <children>
      <VBox fx:id="sideBar" layoutX="-1.0" layoutY="86.0" prefHeight="546.0" prefWidth="146.0" style="-fx-background-color: #777bff; -fx-background-radius: 0 30 30 1;">
         <children>
            <MenuBar fx:id="doctor" prefHeight="50.0" prefWidth="140.0" styleClass="sideMenu" stylesheets="@../css/sideBar.css">
